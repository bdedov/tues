{
  "name": "Sensor Smartphone",
  "description": "Cumulocity Cockpit extension for supporting smartphone connected with TI SensorTag device",
  "ngModules": [
    "c8y.sensorPhone"
  ],
  "js": [
    "cryptojslib/rollups/aes.js",
    "cryptojslib/components/pad-nopadding-min.js",
    "index.js",
    "wizard/crypter.service.js",
    "wizard/phone-registration.service.js",
    "wizard/installation-page.component.js",
    "wizard/registration-page.component.js",
    "dashboards/phone-dashboard.service.js"
